{
    "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
    "dest": "../../dist/ngx-dashboard",
    "lib": {
        "entryFile": "src/public-api.ts",
        "styleIncludePaths": ["src/styles", "src/styles/typography"]
    },
    "assets": [
        "./assets/**/*",
        { "input": "src/styles", "glob": "**/*.scss", "output": "styles" }
    ]
}
